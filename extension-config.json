{
  "version": "1.0.7-beta.1",
  "id": "@shopgate-project/search-doofinder",
  "components": [

  ],
  "configuration": {
    "zone": {
      "type": "admin",
      "destination": "backend",
      "default": "eu1",
      "params": {
        "label": "Doofinder API Zone",
        "type": "text"
      }
    },
    "authKey": {
      "type": "admin",
      "destination": "backend",
      "default": "",
      "params": {
        "label": "Doofinder API Authentication Key",
        "type": "text"
      }
    },
    "hashId": {
      "type": "admin",
      "destination": "backend",
      "default": "",
      "params": {
        "label": "Doofinder Search Index Hash ID",
        "type": "text"
      }
    },
    "productIdKey": {
      "type": "admin",
      "destination": "backend",
      "default": "fallback_reference_id",
      "params": {
        "label": "Doofinder result key representing the product ID",
        "type": "text"
      }
    },
    "filterPriority": {
      "type": "admin",
      "destination": "backend",
      "default": {
        "price": -1,
        "sale_price": -1
      },
      "params": {
        "label": "Doofinder Filter priority (sort position)",
        "type": "object"
      }
    },
    "filterMap": {
      "type": "admin",
      "destination": "backend",
      "default": {
        "brand": "Marke",
        "categories": "Kategorie",
        "price": "Preis",
        "availability": "Verf√ºgbarkeit"
      },
      "params": {
        "label": "Doofinder Filter Map",
        "type": "object"
      }
    }
  }
}
